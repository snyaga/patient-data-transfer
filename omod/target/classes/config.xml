<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>patientdatatransfer</id>
	<name>Patient Data Transfer Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.patientdatatransfer</package>
	<author>susan</author>
	<description>
		Exports and imports patient data
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/patientdatatransfer/update.rdf</updateURL>
	<require_version>1.9.8</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.patientdatatransfer.PatientDataTransferActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.patientdatatransfer.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		PatientDataTransfer.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with patientdatatransfer. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

